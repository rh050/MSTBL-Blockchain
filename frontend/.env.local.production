# MSTBL Production Development Environment
# Use these settings when developing locally against production MSTBL network

# MSTBL Chain Configuration - Production Network
NEXT_PUBLIC_CHAIN_ID=mstbl-1
NEXT_PUBLIC_RPC_ENDPOINT=https://rpc.stbl.mstbl.com
NEXT_PUBLIC_REST_ENDPOINT=https://api.stbl.mstbl.com
NEXT_PUBLIC_BECH32_PREFIX=wasm
NEXT_PUBLIC_COIN_DENOM=stake
NEXT_PUBLIC_COIN_DECIMALS=6

# Application Configuration
NEXT_PUBLIC_APP_NAME=MSTBL Progressive Sale
NEXT_PUBLIC_APP_VERSION=2.0.0

# Contract Addresses (to be updated with actual deployed contracts)
NEXT_PUBLIC_SALE_CONTRACT_ADDRESS=
NEXT_PUBLIC_USDC_CONTRACT_ADDRESS=

# API Configuration - Local Development
NEXT_PUBLIC_API_BASE_URL=http://localhost:3000

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG_LOGS=true
